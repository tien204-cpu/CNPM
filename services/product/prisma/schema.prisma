generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Product {
  id        String   @id @default(cuid())
  name      String
  price     Float
  imageUrl  String?
  description String?
  category  String?
  stock     Int      @default(100)
  createdAt DateTime @default(now())
}
